<Project Sdk="Microsoft.NET.Sdk">
   <PropertyGroup>
      <OutputType>WinExe</OutputType>
      <TargetFramework>net10.0</TargetFramework>
      <Nullable>enable</Nullable>
      <ImplicitUsings>enable</ImplicitUsings>
      <AssemblyName>CodeAIx</AssemblyName>
      <RootNamespace>CodeAIx</RootNamespace>
      <UseWpf>false</UseWpf>
      <UseWindowsForms>false</UseWindowsForms>
   </PropertyGroup>
   <ItemGroup>
      <PackageReference Include="Avalonia" Version="11.0.0" />
      <PackageReference Include="Avalonia.Desktop" Version="11.0.0" />
      <PackageReference Include="Avalonia.Themes.Fluent" Version="11.0.0" />
      <PackageReference Include="Avalonia.Diagnostics" Version="11.0.0" Condition="'$(Configuration)' == 'Debug'" />
      <PackageReference Include="Avalonia.Fonts.Inter" Version="11.0.0" />
      <PackageReference Include="SkiaSharp" Version="2.88.6" />
   </ItemGroup>
   <ItemGroup>
      <None Update="App.axaml">
         <Generator>AvaloniaXaml</Generator>
      </None>
      <None Update="MainWindow.axaml">
         <Generator>AvaloniaXaml</Generator>
      </None>
   </ItemGroup>
</Project>
